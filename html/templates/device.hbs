<!-- header partial -->
{{> header }}

    <!-- Fixed top navbar -->
    {{> topnavbar details_active=true }}

    <!-- Begin page content -->
    <div class="container">

{{> detailstabs device_active=true }}

{{#each device.details}}
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th colspan="2">{{@key}}</th>
    </tr>
  </thead>
  <tbody>
  {{#each this}}
    <tr>
      <td>{{@key}}</td>
      <td>{{this}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
{{/each}}

