<!-- header partial -->
{{> header }}

    <!-- Fixed top navbar -->
    {{> topnavbar diffs_active=true }}

    <!-- Begin page content -->
    <div class="container">

    <h2>Diffs activated</h2>

    {{#each diff}}
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th colspan="2">
        Diff #{{@index}} for 
        {{#each this.path}}
          {{this}}.
        {{/each}}
      </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>LHS</td>
      <td>RHS</td>
    </tr>
    <tr>
      <td>{{this.lhs}}</td>
      <td>{{this.rhs}}</td>
    </tr>
  </tbody>
</table>
<br />
  {{/each}}

<!-- footer partial -->
{{> footer }}
